.main{
width: 100%;
height: 100%;
display: block;
position: relative
}
.maia-img{
  height: 260rpx;
}
.main-tit{
  text-align: center;
  font-size: 32rpx;
  color:#000 ;
  font-weight: bold;
  padding-bottom: 40rpx;
}
.main-ul{
  width: 86%;
  margin: 0 auto;
  background-color: #F5F5F5;
  padding: 30rpx;
  border-radius: 8rpx;
}
.main-ul > .li {
  line-height: 58rpx;
  padding-left: 30rpx;
  position: relative;
}
.main-ul > .li::before{
  content: '';
  width: 10rpx;
  height: 10rpx;
  background-color: #262626;
  border-radius: 50%;
  position: absolute;
  left: 0;
  right: 0;
  top: 24rpx;
}
.main-f{
  padding: 0 30rpx;
  text-align: center;
  color: #bfbfbf;
  bottom: 20rpx; 
  left: 0;
  right: 0;
  position: fixed;      
}
.main-f checkbox-group{
  display: inline-block;
}
.main-f checkbox{
  transform: scale(0.7,0.7);
}
.main-f text{
  color: #262626;
}
.main-f .main-f-btn{
  display: block;
  width: 100%;
  /* padding: 30rpx 0; */
  border-radius: 8rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #07c160;
  margin-top: 10rpx;
}
