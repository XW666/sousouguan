.entry{
 width: 100%;
 height: 100%;
 font-size: 24rpx;
 padding: 0 30rpx;
 box-sizing: border-box;
}
.entry-m{
  position: relative;
  
}
.entry-hr{
  position: absolute;
  left: 4rpx;
    top: 10rpx;
    width: 2rpx;
  height: 100%;
  background-color: rgb(94, 108, 210);
}
.entry-m-t{
  height: 100rpx;
  position: relative;
  padding-left: 26rpx;
  margin-top: 20rpx;
}
.entry-m-t::before{
  content: '';
  width: 12rpx;
  height: 12rpx;
  background-color: rgb(38, 56, 189);
  border-radius: 50%;
  position: absolute;
  left: -1rpx;
  right: 0;
  top:10rpx;
  z-index: 3;
}
.entry-f{
  margin-top: 30rpx;
  text-align: center;
}
.entry-f-btn{
  width:100%;
  height:86rpx;
  background:rgb(94, 108, 210);
  line-height: 86rpx;
  color:white;
   border-radius:10rpx;
   font-size:30rpx;
   margin-top: 20rpx;
}